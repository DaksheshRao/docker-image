pipeline {
  agent any
  stages {
    stage('Build Docker image') {
      steps {
        script {
         def builtImage = docker.build("fibonnaci:1.1", "-f Dockerfile .")
        }
      }
    }
    stage('Push to Docker Hub') {
      steps {
        script {
          docker.withRegistry('https://registry.hub.docker.com', 'docker-dakshesh') 
	  docker.tag('fibonacci:1.1', 'daksheshrao/fibonacci:1.1')
	  {
            builtImage.push()
          }
        }
      }
    }
  }
}

