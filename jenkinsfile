pipeline {
  agent any
  stages {
    stage('Build Docker image') {
      steps {
        script {
          def dockerImage = docker.build("daksheshrao/fibonacci:1.1", "-f Dockerfile .")
        }
      }
    }
    stage('Push to Docker Hub') {
      steps {
        script {
          docker.withRegistry('https://registry.hub.docker.com', 'docker-dakshesh') {
            docker.image('daksheshrao/fibonacci:1.1').push()
          }
        }
      }
    }
    stage('Build and push image on harbor') {
      steps {
        def dockerImage = docker.build('sretraining/daksheshrao-fibonacci:2.0', "-f Dockerfile .")
        docker.withRegistry('https://harbor.cubastion.net', 'harbor_sre')
        dockerImage.push()
       }
      }
    stage('Cleanup') {
      steps {
        cleanWs()
      }
    }
  }
}
